/* Structure of node
class Node
{
    int data;
    Node next;
    Node(int d) {data = d; next = null; }
} */

class Solution {

    // Function to find the data of kth node from
    // the end of a linked list.
    int getKthFromLast(Node head, int k) {
        // Your code here
        int length=1;
        Node temp=head;
        while(temp.next!=null){
            temp=temp.next;
            length++;
        }
        Node fin=head;
        if(length<k){
            return -1;
        }
        for(int i=0;i<length-k;i++){
            fin=fin.next;
        }
        return fin.data;
    }
}
