class Solution {
    public ArrayList<ArrayList<Integer>> transpose(int[][] mat) {
        // code here
        ArrayList<ArrayList<Integer>> res=new ArrayList<>();
        for(int i=0;i<mat.length;i++){
            ArrayList<Integer> l=new ArrayList<>();
            for(int j=0;j<mat[i].length;j++){
                l.add(mat[j][i]);
            }
            res.add(l);
        }
        return res;
    }
}
