class Solution {
    public void swapDiagonal(int[][] mat) {
        // code here
        int n=mat.length;
        for(int i=0;i<mat.length;i++){
            for(int j=0;j<mat[0].length;j++){
                if(i==j){
                    int temp=mat[i][j];
                    mat[i][j]=mat[i][n-1-j];
                    mat[i][n-1-j]=temp;
                }
            }
        }
    }
}
